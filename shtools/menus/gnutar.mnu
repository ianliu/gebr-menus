      <parameter id="n0"><label>Behaviour</label><enum><property required="yes"><keyword></keyword><value/><default></default></property>
        <option>
          <label>create a new archive</label>
          <value>-c</value>
        </option>
        <option>
          <label>extract files from an archive</label>
          <value>-x</value>
        </option>
        <option>
          <label>append tar files to an archive</label>
          <value>-A</value>
        </option>
        <option>
          <label>delete from the archive</label>
          <value>--delete</value>
        </option>
        <option>
          <label>append files to the end of an archive</label>
          <value>-r</value>
        </option>
        <option>
          <label>list the contents of an archive</label>
          <value>-t</value>
        </option>
        <option>
          <label>only append files that are newer than copy in archive</label>
          <value>-u</value>
        </option>
      <option><label>compare archive and file system</label><value>--compare</value></option></enum></parameter>
      <parameter id="n2"><label>Change to directory</label><file directory="yes"><property required="no"><keyword>-C </keyword><value/><default/></property>
      </file></parameter><parameter id="n1"><label>Compression type</label><enum><property required="no"><keyword/><value/><default>-z</default></property>
