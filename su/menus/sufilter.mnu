      </float></parameter>
      <parameter id="n1"><label>Array of filter frequencies</label><float><property required="no" separator=","><keyword>f=</keyword><value/><default/></property></float></parameter>
      <parameter id="n2"><label>Array of filter amplitudes</label><float><property required="no" separator=","><keyword>amps=</keyword><value/><default/></property></float></parameter>
      <parameter id="n3"><label>Echoing useful information</label><flag><property required="no"><keyword>verbose=1</keyword><value></value><default>off</default></property>
        
        
        
      </flag></parameter>
