    <parameters exclusive="0">
      <parameter id="n0"><label>Linear event instead of hyperbolic</label><flag><property required="no"><keyword>type=lmo</keyword><value></value><default>off</default></property>
        
        
        
      </flag></parameter>
      <parameter id="n1"><label>Zero-offset intercept time</label><float><property required="no"><keyword>t0=</keyword><value></value><default>1.0</default></property>
        
        
        
      </float></parameter>
      <parameter id="n2"><label>Moveout velocity in m/s</label><float><property required="no"><keyword>vel=</keyword><value></value><default>3000.0</default></property>
        
        
        
      </float></parameter>
      <parameter id="n3"><label>Amplitude</label><float><property required="no"><keyword>amp=</keyword><value></value><default>1.0</default></property>
        
        
        
      </float></parameter>
      <parameter id="n4"><label>Time sample rate</label><float><property required="no"><keyword>dt=</keyword><value></value><default></default></property>
        
        
        
      </float></parameter>
    </parameters>
  </program>
